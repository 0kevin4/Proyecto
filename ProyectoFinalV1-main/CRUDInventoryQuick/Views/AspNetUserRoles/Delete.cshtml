@model CRUDInventoryQuick.Models.ASPNETUSERROLES

@{
    ViewData["Title"] = "Eliminar";
}

<h1>Eliminar</h1>

<h4>
    ¿Estás segura de que quieres eliminar esto?
</h4>
<div>
    
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.ASPNETUSER_ASPNETUSER)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ASPNETUSER_ASPNETUSER.Id)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.ASPNETROLES_ASPNETROLES)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ASPNETROLES_ASPNETROLES.Id)
        </dd>
    </dl>
    
    <form asp-action="Eliminar">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Eliminar" class="btn btn-sm btn-danger" /> |
        <a asp-action="Index"
            class="btn btn-sm btn-primary">Volver a la lista
        </a>
    </form>
</div>
